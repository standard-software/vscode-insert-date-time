                { "format": "dddd, MMMMM DD, YYYY HH:mm:ss" }
              ]
            },
            {
              "label": "MM DD, YYYY",
              "items": [
                { "format": "MM DD, YYYY" },
                { "format": "ddd, MM DD, YYYY" },
                { "format": "dddd, MM DD, YYYY" },
                {
                  "label": "+Time 12",
                  "separator": true
                },
                { "format": "MM DD, YYYY hh:mm:ss aa" },
                { "format": "ddd, MM DD, YYYY hh:mm:ss aa" },
                { "format": "dddd, MM DD, YYYY hh:mm:ss aa" },
                {
