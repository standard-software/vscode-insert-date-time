                { "format": "ddd, MMMMM DD, YYYY" },
                { "format": "dddd, MMMMM DD, YYYY" }
              ]
            },
            {
              "label": "MM DD, YYYY",
              "items": [
                { "format": "MM DD, YYYY" },
                { "format": "ddd, MM DD, YYYY" },
