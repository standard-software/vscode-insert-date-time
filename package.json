                { "format": "ddd, MMM DD, YYYY HH:mm:ss" },
                { "format": "dddd, MMM DD, YYYY HH:mm:ss" }
              ]
            },
            {
              "label": "MMMMM DD, YYYY",
              "items": [
                { "format": "MMMMM DD, YYYY" },
                { "format": "ddd, MMMMM DD, YYYY" },
                { "format": "dddd, MMMMM DD, YYYY" },
